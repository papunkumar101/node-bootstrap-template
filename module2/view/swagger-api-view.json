{
  "swagger": "2.0",
  "info": {
    "version": "1.0",
    "title": "Module1 Project Management APIs",
    "description": "Documentation"
  },
  "host": "localhost:9001",
  "basePath": "/",
  "tags": [
    {
      "name": "Open",
      "description": "Endpoints"
    },
    {
      "name": "User",
      "description": "User Endpoint"
    },
    {
      "name": "CRUD",
      "description": "CRUD Operation"
    }
  ],
  "schemes": [
    "http",
    "https"
  ],
  "securityDefinitions": {
    "AccessToken": {
      "type": "apiKey",
      "in": "header",
      "name": "x-access-token",
      "description": "Please provide the valid access token, if you don't have please login and get the token as response!"
    },
    "Bearer": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "description": "Enter your bearer token in the format **Bearer &lt;token>**"
    }
  },
  "consumes": [
    "application/json",
    "application/x-www-form-urlencoded"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/api-status": {
      "get": {
        "tags": [
          "Open"
        ],
        "description": "Hello World",
        "parameters": [],
        "responses": {}
      }
    },
    "/v1/mysql-create": {
      "post": {
        "tags": [
          "CRUD"
        ],
        "description": "Insert data in mysql database",
        "parameters": [
          {
            "in": "body",
            "name": "input_name",
            "description": "Enter Name",
            "required": true,
            "schema": {
              "type": "object",
              "required": [
                "input_name"
              ],
              "properties": {
                "input_name": {
                  "type": "string",
                  "example": "Ram"
                }
              }
            }
          }
        ],
        "responses": {}
      }
    }
  },
  "definitions": {
    "UserLogin": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string",
          "example": "papunmahakul1999@gmail.com"
        },
        "password": {
          "type": "string",
          "example": "9e7c5071f8dae2cfb8dc0a0c055cd8ce"
        },
        "username": {
          "type": "string",
          "example": "Papun"
        }
      }
    }
  }
}
